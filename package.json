{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^5.0.12",
    "@types/draftjs-to-html": "^0.8.0",
    "@types/echarts": "^4.6.3",
    "@types/react-draft-wysiwyg": "^1.13.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.7",
    "antd": "3.26.16",
    "axios": "^0.19.2",
    "draftjs-to-html": "^0.9.1",
    "echarts": "^4.8.0",
    "echarts-for-react": "^2.0.16",
    "jsonp": "^0.2.1",
    "less-loader": "5",
    "lint-staged": "^10.0.7",
    "moment": "^2.27.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-bmap": "^1.0.119",
    "react-dom": "^16.12.0",
    "react-draft-wysiwyg": "^1.14.5",
    "umi": "^3.2.7",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@types/jsonp": "^0.2.0",
    "less": "^3.11.3"
  }
}
